# Economic Data API Keys
# Copy this file to .env and add your actual API keys

# World Bank API (optional - public data doesn't require key)
WORLDBANK_API_KEY=your_worldbank_api_key_here

# FRED API (St. Louis Fed) - Get from https://fred.stlouisfed.org/docs/api/api_key.html
FRED_API_KEY=your_fred_api_key_here

# Alpha Vantage (Stock data) - Get from https://www.alphavantage.co/support/#api-key
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key_here

# Project settings
LOG_LEVEL=INFO
DATA_CACHE_DIR=./data/processed/cache
